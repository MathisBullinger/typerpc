{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": [
    "ts",
    "js"
  ],
  "modulePathIgnorePatterns": [
    "<rootDir>/__tests__/utils/*",
    "<rootDir>/lib"
  ],
  "testPathIgnorePatterns": [
    "<rootDir>/__tests__/build"
  ]
}
